CC=gcc
FLAGS=-std=c99 -Wall
TGT=test-

all: clean insertion merge result

clean:
	rm -rf $(TGT)*
	rm -rf *.dat
	rm -rf *.png

insertion:
	$(CC) $(FLAGS) InsertionSort.c -o $(TGT)insertion
	./$(TGT)insertion > InsertionSort.dat
	gnuplot InsertionSort.gp

merge:
	$(CC) $(FLAGS) MergeSort.c -o $(TGT)merge
	./$(TGT)merge > MergeSort.dat
	gnuplot MergeSort.gp

result:
	gnuplot All.gp
